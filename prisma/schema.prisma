
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}


model User {
  id Int @id @default(autoincrement())
  fullname String
  email String @unique
  password String

  data Data[]

  @@map("users")
}


model Data {
  id Int @id @default(autoincrement())
  position String
  budget Int

  userId Int

  user User @relation(fields: [userId], references: [id])

  @@map("datas")
}